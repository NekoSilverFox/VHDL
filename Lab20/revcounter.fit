CHIP "revcounter"
BEGIN

    DEVICE = "EPF10K10LC84-3";

    "aclr"                         : INPUT_PIN  = 42     ;
    "dec"                          : INPUT_PIN  = 84     ;
    "inc"                          : INPUT_PIN  = 2      ;
    "q0"                           : OUTPUT_PIN = 18     ;
    "q1"                           : OUTPUT_PIN = 17     ;
    "q2"                           : OUTPUT_PIN = 16     ;
    "|LPM_COUNTER:14|f8count:p8c0|:6" : LOCATION   = LC1_A1 ;  -- |LPM_COUNTER:14|f8count:p8c0|QC
    "|LPM_COUNTER:14|f8count:p8c0|:7" : LOCATION   = LC3_A1 ;  -- |LPM_COUNTER:14|f8count:p8c0|QB
    "|LPM_COUNTER:14|f8count:p8c0|:8" : LOCATION   = LC5_A1 ;  -- |LPM_COUNTER:14|f8count:p8c0|QA
    ":32"                          : LOCATION   = LC2_A1 ;

END;

INTERNAL_INFO "revcounter"
BEGIN
	DEVICE = EPF10K10LC84-3;
    OD3P42  : LORAX = "1:FB3->LC1_A1,->LC3_A1,->LC5_A1|";
    OD0P84  : LORAX = "1:FB0|2:FH0R0,PA0R0C0->LC1_A1,->LC3_A1,->LC2_A1||";
    OD1P2   : LORAX = "1:FB1|2:FH1R0,PA20R0C0->LC2_A1||";
    LC1_A1  : LORAX = "1:MSW0R0C0,HHL22R0->OH8R0P16|";
    LC3_A1  : LORAX = "1:MSW2R0C0,HHL3R0->OH10R0P17|";
    LC5_A1  : LORAX = "1:MSW4R0C0,HHL41R0->OH12R0P18|";
	LC1_A1  : LORAX2 = "LC1_A1, LC3_A1, LC5_A1, OD0P84";
	LC3_A1  : LORAX2 = "X, LC3_A1, LC5_A1, OD0P84";
	LC5_A1  : LORAX2 = "X, X, X, LC5_A1";
	LC2_A1  : LORAX2 = "X, X, OD1P2, OD0P84";
END;
